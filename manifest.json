{
  "manifest_version": 2,
  "name": "Show weekends on Asana",
  "version": "1.0.0",
  "description": "Show weekends on Asana.",
  "page_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_title": "Ext title",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://app.asana.com/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}
